{
  "name": "eth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "yarn hardhat compile",
    "typechain": "yarn hardhat typechain",
    "build": "run-s compile typechain",
    "chain": "yarn hardhat node",
    "clean": "rm -rf build artifacts typechain-types cache networks abi",
    "fmt:ts": "prettier scripts/*.ts scripts/**/*.ts test/*.ts test/**/*.ts hardhat.config.ts --write",
    "fmt:sol": "prettier contracts/*.sol contracts/**/*.sol --write",
    "fmt": "run-s fmt:ts fmt:sol",
    "lint:ts": "eslint 'scripts/**/*.ts' 'hardhat.config.ts'",
    "lint:tsfix": "eslint 'scripts/**/*.ts' 'hardhat.config.ts' --fix",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:solfix": "solhint 'contracts/**/*.sol' --fix",
    "lint": "run-s lint:ts lint:sol",
    "lint:fix": "run-s lint:tsfix lint:solfix",
    "hardhat:loc": "yarn hardhat --network local",
    "hardhat:qa03": "yarn hardhat --network qa03",
    "hardhat:polygon": "yarn hardhat --network polygon-mumbai",
    "hardhat:avax": "yarn hardhat --network avax-fuji",
    "hardhat:bnc": "yarn hardhat --network bnb-testnet",
    "hardhat:gsctest": "yarn hardhat --network gsc-testnet",
    "hardhat:anvil": "yarn hardhat --network anvil",
    "hardhat:goerli": "yarn hardhat --network goerli",
    "dep1:loc": "yarn hardhat:loc run src/deploy1.ts",
    "dep1:qa03": "yarn hardhat:qa03 run src/deploy1.ts",
    "dep1:anvil": "yarn hardhat:anvil run src/deploy1.ts",
    "dep1:polygon": "yarn hardhat:polygon run src/deploy1.ts",
    "dep1:avax": "yarn hardhat:avax run src/deploy1.ts",
    "dep1:bnc": "yarn hardhat:bnc run src/deploy1.ts",
    "dep1:goerli": "yarn hardhat:goerli run src/deploy1.ts",
    "dep2:loc": "yarn hardhat:loc run src/deploy1.ts",
    "dep2:gsctest": "yarn hardhat:gsctest run src/deploy2.ts",
    "dep2:anvil": "yarn hardhat:anvil run src/deploy2.ts",
    "dep2:goerli": "yarn hardhat:goerli run src/deploy2.ts",
    "fg:loc": "yarn hardhat:loc fg",
    "fg:qa03": "yarn hardhat:qa03 fg",
    "fg:anvil": "yarn hardhat:anvil fg",
    "fg:polygon": "yarn hardhat:polygon fg",
    "fg:avax": "yarn hardhat:avax fg",
    "fg:bnc": "yarn hardhat:bnc fg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "hardhat": "^2.12.7"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "chai": "^4.2.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  }
